<?xml version="1.0" encoding="UTF-8"?>
<fractals>
    <fractal>
        <name>Dragon Curve</name>
        <source>https://en.wikipedia.org/wiki/Dragon_curve</source>
        <axiom>FX</axiom>
        <rules>
            <rule replace="X" with="X+YF+"/>
            <rule replace="Y" with="-FX-Y"/>
        </rules>
        <angle>90</angle>
        <forward-symbols>F</forward-symbols>
    </fractal>
    <fractal>
        <name>Dragon Curve: 3</name>
        <source>http://larryriddle.agnesscott.org/ifs/heighway/heighway.htm</source>
        <axiom>FX</axiom>
        <rules>
            <rule replace="F" with=""/>
            <rule replace="X" with="+FX--FY+"/>
            <rule replace="Y" with="-FX++FY-"/>
        </rules>
        <angle>45</angle>
        <forward-symbols>F</forward-symbols>
    </fractal>
    <fractal>
        <name>Twin Dragon</name>
